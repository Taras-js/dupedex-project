{
  "name": "dupedex",
  "version": "1.0.0",
  "description": "APP from react, next, mongoDB, express, node",
  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node app.js",
    "server": "nodemon app.js",
    "client": "npm run dev --prefix client",
    "client:install": "npm install --prefix client",
    "client:build": "npm run build --prefix client",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Taras-js/dupedex-project.git"
  },
  "keywords": [
    "express",
    "node",
    "react",
    "mongoDB"
  ],
  "author": "Taras Schevchenko <shev-taras@ya.ru>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Taras-js/dupedex-project/issues"
  },
  "homepage": "https://github.com/Taras-js/dupedex-project#readme",
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.2",
    "config": "^3.3.7",
    "express": "^4.17.3",
    "express-validator": "^6.14.0",
    "fs": "^0.0.1-security",
    "http-proxy-middleware": "^2.0.4",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongoose": "^6.2.7",
    "multer": "^1.4.4",
    "path": "^0.12.7",
    "react-redux": "^7.2.8",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.1",
    "shortid": "^2.2.16"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.15",
    "prettier": "2.6.1"
  }
}
